<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.control.TreeView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.Priority?>
<?import com.jfoenix.controls.JFXButton?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.control.TextField?>
<fx:root xmlns="http://javafx.com/javafx"
         type="BorderPane" stylesheets="@setting.css"
         xmlns:fx="http://javafx.com/fxml"
         prefHeight="400.0" prefWidth="600.0">
    <center>
        <HBox alignment="CENTER">
            <Label text="Send Failed Limit Count : "/>
            <JFXTextField fx:id="countField"/>
        </HBox>
    </center>
    <bottom>
        <HBox alignment="CENTER">
            <JFXButton text="Save" prefWidth="100" styleClass="choice-button" fx:id="saveBtn"/>
            <Label prefWidth="20"/>
            <JFXButton text="Cancel" prefWidth="100" styleClass="choice-button" fx:id="cancelBtn"/>
        </HBox>
    </bottom>
</fx:root>
